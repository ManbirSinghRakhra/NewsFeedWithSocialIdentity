@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>


<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dt>@claim.Value</dt>
        <dt>@claim.Issuer</dt>
        <dt>@claim.OriginalIssuer</dt>
        <dt>@claim.Properties</dt>

    }
</dl>
